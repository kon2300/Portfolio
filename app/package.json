{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "css": "TAILWIND_MODE=watch postcss ./src/index.css -o ./src/dist.css -w",
    "build": "NODE_ENV=production vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test:unit": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.4",
    "axios": "^0.21.4",
    "chart.js": "^3.5.1",
    "lodash": "^4.17.21",
    "vee-validate": "^4.4.11",
    "vue": "^3.0.5",
    "vue-router": "4",
    "vuex": "^4.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.3.3",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "^27.4.7",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.8",
    "vite": "^2.4.4",
    "vue-jest": "^3.0.7"
  }
}
